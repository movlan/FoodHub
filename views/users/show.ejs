<%- include('../partials/header') %> 
<h1><%= title %></h1>
<% if (recipes.length) { %>
    <% recipes.forEach(recipe => { %>
        <h3><%= recipe.name%> </h3>
        <a href="/recipes/<%= recipe._id %>">More</a>
    <% }) %> 
<% } else { %>
    <h3>No Recipes Yet</h3>
<% }; %>

<%- include('../partials/footer') %> 